

PCOLOR=$(echo $PCOLOR |  grep -o '[[:digit:]]*' | cut -c 4)

case $(tty) in /dev/tty[0-9]*)
     SEP="|"
     PROMPTCHAR=">"
     X="X "
     source ~/.config/zsh/zsh-functions/transientprompt
   ;;
  *)
     SEP=""
     PROMPTCHAR="›"
     X="窱"
     source ~/.config/zsh/zsh-functions/transientprompt
   ;;
esac
